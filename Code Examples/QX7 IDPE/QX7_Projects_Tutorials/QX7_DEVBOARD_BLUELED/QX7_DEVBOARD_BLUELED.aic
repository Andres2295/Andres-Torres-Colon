//PROGRAM TITLE: MICROSTICK II DEVELOPMENT BOARD BLUE LED
//PURPOSE: ACTIVATE AND CONTROL ONE OF THE BLUE LEDS
//AUTHOR: ANDES TORRES COLON
//DATE: MARCH 22, 2020

void setup(void)
{
   //INITIALIZE IO PINS	
   digitalSetup(26, OUTPUT);
   digitalWrite(26, HIGH);
   
}

void loop(void)
{
   //PROGRAM LOGISTICS	
   digitalWrite(26, HIGH);
   delay(1000);
   digitalWrite(26, LOW);
   delay(1000);
}
